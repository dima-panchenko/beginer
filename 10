#include <iostream>
# include <cmath>
using namespace std;

//Даны два ненулевых числа. Найти сумму, разность, произведение и
//частное их квадратов.
int main()
{
    int s,d,f,g;
    int a;
    int b;
    cin>> a;
    cin>> b;
   int r=a*a;
   int t=b*b;
   s=r+t;
    d=r-t;
    f=r*t;
    g=r/t;
    cout<< "сумма="<< s<< endl;
    cout<<"разность="<<d<<endl;
    cout<<"произведение="<<f<<endl;
    cout<<"частное="<<g<<endl;
    return 0;
}
